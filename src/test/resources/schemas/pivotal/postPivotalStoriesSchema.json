{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "kind",
    "id",
    "project_id",
    "name",
    "story_type",
    "current_state",
    "requested_by_id",
    "owner_ids",
    "labels",
    "created_at",
    "updated_at",
    "url"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer"
    },
    "project_id": {
      "$id": "#/properties/project_id",
      "type": "integer"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string"
    },
    "story_type": {
      "$id": "#/properties/story_type",
      "type": "string"
    },
    "current_state": {
      "$id": "#/properties/current_state",
      "type": "string"
    },
    "requested_by_id": {
      "$id": "#/properties/requested_by_id",
      "type": "integer"
    },
    "owner_ids": {
      "$id": "#/properties/owner_ids",
      "type": "array",
      "items": {
        "$id": "#/properties/owner_ids/items"
      }
    },
    "labels": {
      "$id": "#/properties/labels",
      "type": "array",
      "items": {
        "$id": "#/properties/labels/items"
      }
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string"
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string"
    }
  }
}
